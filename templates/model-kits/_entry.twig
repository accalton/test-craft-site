{% extends "_layouts/main" %}

{% block content %}
    <div class="kit-wrapper">
        <h1>{{ entry.title }}</h1>
        <h2>{{ entry.productLine.level(1).one().title }}</h2>
        <h3>{{ entry.productLine.level(2).one().title }}</h3>
        <img src="{{ entry.coverImage.one().url }}" alt="">
    </div>
{% endblock %}